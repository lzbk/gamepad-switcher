[xboxdrv]
ui-clear=true
trigger-as-button = true

[ui-axismap]
x1=KEY_LEFT:KEY_RIGHT
#y1=KEY_UP:KEY_DOWN
x2^dead:6000 = REL_X:750:-1
y2^dead:6000 = REL_Y:750:-1

# trigger^invert = rel-repeat:REL_WHEEL:1:50

[ui-buttonmap]
a = KEY_SPACE
x = KEY_ENTER

lb = KEY_LEFT
rb = KEY_RIGHT

[ui-buttonmap]
du = KEY_UP
dr = KEY_RIGHT
dd = KEY_DOWN
dl = KEY_LEFT

[ui-buttonmap]
start = KEY_ESC
back  = KEY_TAB

